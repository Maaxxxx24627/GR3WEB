# Activer le moteur de réécriture si pas déjà fait
RewriteEngine On

# Accepter le chargement des fichiers et répertoires réels
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rediriger tout le reste vers la page index
RewriteRule ^(.*)$ index.php?url=$1 [L,QSA]

<IfModule mod_headers.c>
    Header set Access-Control-Allow-Origin "*"
</IfModule>

# Restriction d'accès par IP
<RequireAll>
    Require ip 192.168.56.1
</RequireAll>
